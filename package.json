{
  "name": "arnie",
  "version": "0.1.1",
  "description": "Action hero for backends",
  "main": "dist/index.js",
  "scripts": {
    "test": "tape -r babel-register 'test/**/*.js'",
    "test:watch": "watch 'npm test' --ignoreDirectoryPattern='/node_modules/i' --ignoreDotFiles",
    "start": "nodemon demo/src/main.js --exec babel-node",
    "build": "babel src -d dist",
    "lint": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxfrigge/arnie.git"
  },
  "author": "Max Frigge <max.frigge@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maxfrigge/arnie/issues"
  },
  "homepage": "https://github.com/maxfrigge/arnie#readme",
  "dependencies": {
    "check-types": "7.0.0",
    "get-value": "2.0.6",
    "path-to-regexp": "1.5.3"
  },
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-eslint": "6.1.2",
    "babel-preset-es2015-node6": "0.3.0",
    "babel-preset-stage-3": "6.11.0",
    "babel-register": "6.11.6",
    "koa": "2.0.0-alpha.3",
    "standard": "7.1.2",
    "supertest": "2.0.0",
    "tape": "4.6.0",
    "watch": "0.19.1"
  },
  "standard": {
    "parser": "babel-eslint"
  }
