{
  "name": "arnie",
  "version": "0.2.6",
  "description": "Action hero for backends",
  "main": "index.js",
  "scripts": {
    "lint": "standard 'src/**/*.js' 'test/**/*.js'",
    "pretest-disabled": "npm run lint",
    "test": "tape 'test/**/*.js'",
    "test:watch": "watch 'npm test' --ignoreDirectoryPattern='/node_modules/i' --ignoreDotFiles",
    "prebuild": "npm test",
    "build": "rimraf lib && BABEL_ENV=production babel src/ --out-dir=lib/",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxfrigge/arnie.git"
  },
  "author": "Max Frigge <max.frigge@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maxfrigge/arnie/issues"
  },
  "homepage": "https://github.com/maxfrigge/arnie#readme",
  "dependencies": {
    "accepts": "^1.3.3",
    "content-disposition": "^0.5.1",
    "content-type": "^1.0.2",
    "escape-html": "^1.0.3",
    "fresh": "^0.3.0",
    "get-value": "^2.0.6",
    "koa-is-json": "^1.0.0",
    "mime-types": "^2.1.12",
    "set-value": "^0.4.0",
    "statuses": "^1.3.0",
    "type-is": "^1.6.13",
    "vary": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "rimraf": "^2.5.4",
    "standard": "^8.5.0",
    "supertest": "^2.0.1",
    "tape": "^4.6.2",
    "watch": "^1.0.1"
  }
}
